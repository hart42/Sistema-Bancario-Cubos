{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-restricted-syntax":"off",
        "no-use-before-define":"off",
        "no-shadow":"off",
        "no-plusplus":"off",
        "max-len":"off"
    }
}
